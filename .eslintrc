{
  "extends": "airbnb-base",
  "env": {
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "comma-dangle": 0,
    "prefer-arrow-callback": 0,
    "class-methods-use-this": 0,
    "func-names": 0,
    "newline-before-return": 2,
    "no-prototype-builtins": 0,
    "implicit-arrow-linebreak": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "operator-linebreak": 0
  }
}
